{
  "name"          : "backbone",
  "description"   : "Give your JS App some Backbone with Models, Views, Collections, and Events.",
  "url"           : "http://backbonejs.org",
  "keywords"      : ["model", "view", "controller", "router", "server", "client", "browser"],
  "author"        : "Jeremy Ashkenas",
  "dependencies"  : {
    "underscore"  : ">=1.8.3"
  },
  "devDependencies": {
    "eslint": "1.10.x",
    "karma": "^0.13.13",
    "karma-phantomjs-launcher": "^0.1.4",
    "karma-qunit": "^0.1.5",
    "qunitjs": "^1.18.0",
  },
  "scripts": {
    "test": "karma start && npm run lint",
    "build": "uglifyjs backbone.js --mangle --source-map backbone-min.map -o backbone-min.js",
    "lint": "eslint backbone.js test/*.js"
  },
  "main"          : "backbone.js",
  "version"       : "1.2.3",
  "license"       : "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/jashkenas/backbone.git"
  },
  "files": [
    "backbone.js", "backbone-min.js", "backbone-min.map", "LICENSE"
  ]
}

